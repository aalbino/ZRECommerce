msgid "'%value%' is less than %min% characters long"
msgstr "'%value%' es menos de %min% letras de largo"

msgid "'%value%' is greater than %max% characters long"
msgstr "'%value%' es mas de %max% letras de largo"

msgid "'%value%' is not a valid email address in the basic format local-part@hostname"
msgstr "'%value%' no es un correo electrónico en un formato basico como 'parte-local@dominio'"

